{
  "name": "thee",
  "version": "1.0.0",
  "description": "Micro-utility to shift the `this` context parameter of functions into the first regular parameter position.",
  "main": "thee.js",
  "scripts": {
    "test": "mocha --compilers js:babel/register test",
    "test:watch": "mocha --watch --compilers js:babel/register test",
	"build": "babel thee.js --modules umd --out-file thee.es5.js",
	"build:watch": "babel thee.js --modules umd --watch --out-file thee.es5.js"
  },
  "keywords": [
    "this",
    "scope",
    "bind",
    "apply",
    "utility",
    "object",
    "function",
    "context"
  ],
  "author": "gmauer@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.2.17",
    "mocha": "^2.2.4",
    "should": "^6.0.1"
  },
  "dependencies": {
  },
  "repository": {
	  "type": "git",
	  "url": "https://github.com/togakangaroo/thee"
  }
}
